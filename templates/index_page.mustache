{{!
    @template local_hackfest/index_page

    Demonstrates how templates are used.
}}
<div data-region="index-page">
    {{#thispagerocks}}
        <strong>{{#str}}doesrock, local_hackfest{{/str}}</strong>
    {{/thispagerocks}}
    {{^thispagerocks}}
        <strong>{{#str}}doesnotrock, local_hackfest{{/str}}</strong>
    {{/thispagerocks}}
    <div id="footer" style="margin-top:100px">
        {{strfooter}}
    </div>
</div>
